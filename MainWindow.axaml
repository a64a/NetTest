<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sp="clr-namespace:ScottPlot.Avalonia;assembly=ScottPlot.Avalonia"
        x:Class="NetTest.MainWindow"
        Icon="Assets/NetTest.ico"
        Title="NetTest" Width="500" Height="500">
    <StackPanel Margin="10">
        <ToggleButton x:Name="networkTestButton" Content="Start Network Test" Checked="OnStartTest" Unchecked="OnStopTest" Foreground="White" Margin="0 0 0 10"/>
        <TextBlock x:Name="messageGoogleDNS" TextWrapping="Wrap" Margin="10" Height="100"/>
        <TextBlock x:Name="messageGateway" TextWrapping="Wrap" Margin="10" Height="100"/>
        <sp:AvaPlot x:Name="pingPlot" Height="200" Margin="10" Background="#FFFFFF"/>
    </StackPanel>
</Window>
